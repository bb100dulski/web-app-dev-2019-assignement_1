{{#each albums}}
  <section class="ui segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Total Size: {{size}} </p>
    <a href="/album/{{id}}"> <i class="ui folder open icon"></i> </a>
    <a href="/dashboard/deleteAlbum/{{id}}"> <i class="ui trash icon"></i> </a>
  </section>
{{/each}}